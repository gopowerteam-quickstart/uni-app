{
    "name": "uni-preset-vue",
    "version": "0.0.0",
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "scripts": {
        "doc": "docsify serve docs",
        "update:request": "node ./scripts/generate-request/index.js",
        "log": "conventional-changelog -i README.md -s -r 0",
        "commit": "node ./node_modules/cz-customizable/standalone.js",
        "prepare": "husky install",
        "analyzer": "cross-env use_analyzer=true npm run build:mp-weixin",
        "uni-upgrade": "node ./scripts/uni-upgrade/index.js",
        "pretty": "prettier --write \"./src/**/*.{js,ts,vue}\"",
        "lint": "eslint --fix --ext .ts,.tsx,.js,.vue ./src",
        "dev:app": "uni -p app",
        "dev:custom": "uni -p",
        "dev:h5": "uni",
        "dev:h5:ssr": "uni --ssr",
        "dev:mp-alipay": "uni -p mp-alipay",
        "dev:mp-baidu": "uni -p mp-baidu",
        "dev:mp-kuaishou": "uni -p mp-kuaishou",
        "dev:mp-lark": "uni -p mp-lark",
        "dev:mp-qq": "uni -p mp-qq",
        "dev:mp-toutiao": "uni -p mp-toutiao",
        "dev:mp-weixin": "uni -p mp-weixin",
        "dev:quickapp-webview": "uni -p quickapp-webview",
        "dev:quickapp-webview-huawei": "uni -p quickapp-webview-huawei",
        "dev:quickapp-webview-union": "uni -p quickapp-webview-union",
        "build:app": "uni build -p app",
        "build:custom": "uni build -p",
        "build:h5": "uni build",
        "build:h5:ssr": "uni build --ssr",
        "build:mp-alipay": "uni build -p mp-alipay",
        "build:mp-baidu": "uni build -p mp-baidu",
        "build:mp-kuaishou": "uni build -p mp-kuaishou",
        "build:mp-lark": "uni build -p mp-lark",
        "build:mp-qq": "uni build -p mp-qq",
        "build:mp-toutiao": "uni build -p mp-toutiao",
        "build:mp-weixin": "uni build -p mp-weixin",
        "build:quickapp-webview": "uni build -p quickapp-webview",
        "build:quickapp-webview-huawei": "uni build -p quickapp-webview-huawei",
        "build:quickapp-webview-union": "uni build -p quickapp-webview-union"
    },
    "dependencies": {
        "@dcloudio/uni-app": "3.0.0-alpha-3031020220124001",
        "@dcloudio/uni-app-plus": "3.0.0-alpha-3031020220124001",
        "@dcloudio/uni-components": "3.0.0-alpha-3031020220124001",
        "@dcloudio/uni-h5": "3.0.0-alpha-3031020220124001",
        "@dcloudio/uni-mp-alipay": "3.0.0-alpha-3031020220124001",
        "@dcloudio/uni-mp-baidu": "3.0.0-alpha-3031020220124001",
        "@dcloudio/uni-mp-kuaishou": "3.0.0-alpha-3031020220124001",
        "@dcloudio/uni-mp-lark": "3.0.0-alpha-3031020220124001",
        "@dcloudio/uni-mp-qq": "3.0.0-alpha-3031020220124001",
        "@dcloudio/uni-mp-toutiao": "3.0.0-alpha-3031020220124001",
        "@dcloudio/uni-mp-weixin": "3.0.0-alpha-3031020220124001",
        "@dcloudio/uni-quickapp-webview": "3.0.0-alpha-3031020220124001",
        "@vueuse/core": "7.5.5",
        "class-transformer": "^0.5.1",
        "dayjs": "1.10.7",
        "pinia": "2.0.9",
        "qiniu-js": "3.4.0",
        "qs": "6.10.3",
        "rxjs": "7.5.2",
        "vue": "3.2.26"
    },
    "devDependencies": {
        "@commitlint/cli": "16.1.0",
        "@commitlint/config-conventional": "16.0.0",
        "@dcloudio/types": "2.5.16",
        "@dcloudio/uni-automator": "3.0.0-alpha-3031020220124001",
        "@dcloudio/uni-cli-shared": "3.0.0-alpha-3031020220124001",
        "@dcloudio/vite-plugin-uni": "3.0.0-alpha-3031020220124001",
        "@prettier/plugin-pug": "1.19.2",
        "@types/qs": "6.9.7",
        "@types/svgo": "^2.6.1",
        "@typescript-eslint/eslint-plugin": "5.10.1",
        "@typescript-eslint/parser": "5.10.1",
        "@vitejs/plugin-vue-jsx": "1.3.3",
        "@vue/eslint-config-prettier": "7.0.0",
        "@vue/eslint-config-typescript": "10.0.0",
        "autoprefixer": "10.4.2",
        "cz-customizable": "6.3.0",
        "docsify-cli": "4.4.3",
        "eslint": "8.7.0",
        "eslint-plugin-vue": "8.3.0",
        "handlebars": "4.7.7",
        "hjson": "3.2.2",
        "husky": "7.0.4",
        "lint-staged": "12.3.1",
        "node-fetch": "2",
        "postcss": "8.4.5",
        "prettier": "2.5.1",
        "pug": "3.0.2",
        "sass": "1.49.0",
        "sass-loader": "12.4.0",
        "svgo": "2.8.0",
        "tailwindcss": "3.0.16",
        "tailwindcss-miniprogram-preset": "2.1.1",
        "typescript": "4.5.5",
        "unplugin-auto-import": "0.5.11",
        "vite": "2.7.13",
        "vite-plugin-eslint": "1.3.0",
        "vite-svg-loader": "3.1.2"
    },
    "uni-app": {
        "version": "3.2.16.20211122",
        "scripts": {}
    }
}
