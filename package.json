{
  "name": "uni-preset-vue",
  "version": "0.0.0",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "scripts": {
    "update:request": "npx request-generate",
    "postupdate:request": "prettier --write \"./src/http/**/*.ts\"",
    "log": "conventional-changelog -i README.md -s -r 0",
    "commit": "node ./node_modules/cz-customizable/standalone.js",
    "prepare": "husky install",
    "analyzer": "cross-env use_analyzer=true npm run build:mp-weixin",
    "uni-upgrade": "node ./scripts/uni-upgrade/index.js",
    "lint": "eslint --fix --ext .ts,.tsx,.js,.vue ./src",
    "format": "prettier --config .prettierrc-cli.js --write \"./src/**/*.{js,ts,vue}\"",
    "dev:app": "uni -p app",
    "dev:custom": "uni -p",
    "dev:h5": "uni",
    "dev:h5:ssr": "uni --ssr",
    "dev:mp-weixin": "uni -p mp-weixin",
    "build:app": "uni build -p app",
    "build:custom": "uni build -p",
    "build:h5": "uni build",
    "build:h5:ssr": "uni build --ssr",
    "build:mp-weixin": "uni build -p mp-weixin"
  },
  "dependencies": {
    "@dcloudio/uni-app": "3.0.0-3070320230222002",
    "@dcloudio/uni-app-plus": "3.0.0-3070320230222002",
    "@dcloudio/uni-components": "3.0.0-3070320230222002",
    "@dcloudio/uni-h5": "3.0.0-3070320230222002",
    "@dcloudio/uni-mp-weixin": "3.0.0-3070320230222002",
    "@gopowerteam/request": "^0.1.13",
    "@vueuse/core": "9.13.0",
    "@vueuse/shared": "9.13.0",
    "dayjs": "1.11.7",
    "pinia": "2.0.32",
    "qs": "6.9.7",
    "vue": "3.2.47"
  },
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@babel/eslint-parser": "7.19.1",
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@dcloudio/types": "^3.2.11",
    "@dcloudio/uni-automator": "3.0.0-3070320230222002",
    "@dcloudio/uni-cli-shared": "3.0.0-3070320230222002",
    "@dcloudio/uni-stacktracey": "3.0.0-3070320230222002",
    "@dcloudio/vite-plugin-uni": "3.0.0-3070320230222002",
    "@gopowerteam/eslint-config": "^0.0.7",
    "@gopowerteam/request-generate": "^0.1.13",
    "@types/qs": "6.9.7",
    "@types/svgo": "2.6.4",
    "@unocss/core": "^0.50.1",
    "@vitejs/plugin-vue-jsx": "3.0.0",
    "@vue/babel-preset-app": "^5.0.8",
    "commitizen": "4.3.0",
    "cz-customizable": "7.0.0",
    "eslint": "8.35.0",
    "hjson": "3.2.2",
    "husky": "8.0.3",
    "lint-staged": "13.1.2",
    "nanoid": "4.0.1",
    "prettier": "2.8.4",
    "qiniu": "^7.8.0",
    "rimraf": "^4.1.2",
    "sass": "^1.62.1",
    "svgo": "3.0.2",
    "tslib": "2.5.0",
    "typescript": "4.9.5",
    "unocss": "0.50.1",
    "unocss-preset-weapp": "0.4.2",
    "unplugin-auto-import": "0.15.0",
    "vite": "4.1.4",
    "vite-plugin-eslint": "1.8.1",
    "vite-svg-loader": "4.0.0",
    "vue-demi": "^0.13.11"
  }
}
